[access]
	inheritFrom = All-Projects
[project]
	description = MariaDB server is a community developed fork of MySQL server. Started by core members of the original MySQL team, MariaDB actively works with outside developers to deliver the most featureful, stable, and sanely licensed open SQL server in the industry.
[access "refs/*"]
	owner = group user/Srikrishan Malik (srimalik)
[access "refs/for/refs/*"]
	read = group user/Srikrishan Malik (srimalik)
	push = group user/Srikrishan Malik (srimalik)
	label-Code-Review = -2..+2 group user/Srikrishan Malik (srimalik)
	label-Verified = -1..+1 group user/Srikrishan Malik (srimalik)
	rebase = group user/Srikrishan Malik (srimalik)
	removeReviewer = group user/Srikrishan Malik (srimalik)
	submit = group user/Srikrishan Malik (srimalik)
[access "refs/heads/*"]
	read = group user/Srikrishan Malik (srimalik)
	create = group user/Srikrishan Malik (srimalik)
	push = +force group user/Srikrishan Malik (srimalik)
	pushMerge = group user/Srikrishan Malik (srimalik)
[access "refs/tags/*"]
	push = group user/Srikrishan Malik (srimalik)
	push = +force group user/Srikrishan Malik (srimalik)
